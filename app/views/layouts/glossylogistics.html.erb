							<hgroup class="element-invisible">
								<h1 id="site-name" class="element-invisible"><a href="http://www.amqp.org/" rel="home" title="Home page" class="active">AMQP</a></h1>
							</hgroup>
						</div>
						<div id="header-end-blocks">
							<div class="region region-header-end">
								<nav id="block-superfish-1" class="block block-superfish block-odd block-header-end block-count-3" role="navigation">
									<div class="block-inner">
										<div class="block-content">
											<%= render :partial => 'menus' %>
										</div>
									</div>
								</nav>
							</div>
						</div>
					</header>
					<!-- /header -->
					<% if request[:action].eql? 'index' %>
					<%= render :partial => 'slider_skin' %>
					<% end %>
				</div>
				<!-- /top-inner -->
			</div>
			<!-- /top-wrapper -->
			<% if request[:action].eql? 'index' %>
			<%= render :partial => 'slider_navigation' %>
			<% end %> <div id="breadcrumb-wrapper"></div>
			<div id="content-wrapper">
