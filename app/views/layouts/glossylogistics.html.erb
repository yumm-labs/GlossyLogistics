	<body id="genesis-2c">
		<div id="skip-link">
			<a href="#main-content">Skip to main content</a>
		</div>
		<div id="container" class="html front not-logged-in one-sidebar sidebar-first page-node page-node- page-node-7 node-type-page">
			<div id="top-wrapper">
				<div id="top-inner">
					<header id="header" class="clearfix" role="banner">
						<div id="branding">
							<div id="logo">
								<%= link_to root_url do %>
								<%= image_tag 'glossy_logo.png', :alt => "Glossy Logistics logo", :height => '105px' %>
								<% end %>
								<%= content_tag :div, 'a logistics company', :id => 'caption' %>
